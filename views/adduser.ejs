<%- include layouts/header.ejs %>

	<div class="page-data">
		<form method="post" action="/users/add">
			<table cellpadding="11">
				<tr>
					<td class="label">Name</td><td>:<input type="text" name="name" placeholder="ex.pandu" required></td>

				</tr>
				<tr>
					<td class="label">Email</td><td>:<input type="text" name="email" placeholder="ex.pandu560@gmail.com" required></td>
				</tr>
				<tr>
					<td class="label">Mobileno</td><td>:<input type="text" name="mobileno" placeholder="ex.9876543210" required></td>
				</tr>
				<tr>
					<td class="label">City</td><td>:<input type="text" name="city" placeholder="ex.Ahmedabad" required></td>
				</tr>
				<tr>
					<td class="label">State</td><td>:<input type="text" name="state" placeholder="ex.Gujrath" required></td>
				</tr>
				<tr>
					<td class="label">Country</td><td>:<input type="text" name="country" placeholder="ex.India" required></td>
				</tr>
				<tr>
					<td class="label">Zipcode</td><td>:<input type="text" name="zipcode" placeholder="ex.380023" required></td>
				</tr>
				<tr>
                    <td class="label"></td>
                    <td>
                        &nbsp; <input type="submit" name="submit" value="Save">
                       <input type="button" name="cancel" value="Cancel" class="cancel" onClick="cancelAdd()">
                    </td>
                 </tr>
			</table>
		</form>
	</div>

<%- include layouts/footer.ejs %>